{
  "title": "Queue & Audit Metrics",
  "panels": [
    {
      "type": "stat",
      "title": "Jobs Enqueued",
      "targets": [{ "expr": "parse_jobs_enqueued_total" }],
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 4 }
    },
    {
      "type": "stat",
      "title": "Jobs Completed",
      "targets": [{ "expr": "parse_jobs_completed_total" }],
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 4 }
    },
    {
      "type": "stat",
      "title": "Jobs Failed",
      "targets": [{ "expr": "parse_jobs_failed_total" }],
      "gridPos": { "x": 16, "y": 0, "w": 8, "h": 4 }
    },
    {
      "type": "stat",
      "title": "Queue Waiting",
      "targets": [{ "expr": "parse_queue_waiting" }],
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 4 }
    },
    {
      "type": "stat",
      "title": "Queue Active",
      "targets": [{ "expr": "parse_queue_active" }],
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 4 }
    },
    {
      "type": "graph",
      "title": "Job Duration p50/p90/p99 (est.)",
      "targets": [
        { "expr": "histogram_quantile(0.5, sum(rate(parse_job_duration_seconds_bucket[5m])) by (le))" },
        { "expr": "histogram_quantile(0.9, sum(rate(parse_job_duration_seconds_bucket[5m])) by (le))" },
        { "expr": "histogram_quantile(0.99, sum(rate(parse_job_duration_seconds_bucket[5m])) by (le))" }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 }
    }
  ],
  "schemaVersion": 38,
  "version": 1,
  "editable": true
}